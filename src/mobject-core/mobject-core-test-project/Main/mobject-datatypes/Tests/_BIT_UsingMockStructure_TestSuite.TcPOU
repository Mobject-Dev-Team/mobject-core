    </Implementation>
    <Method Name="TestAddress" Id="{1d27ee1f-0b09-4785-b8d2-bcd3ab86453c}">
      <Declaration><![CDATA[METHOD PUBLIC TestAddress
VAR
    _type : _MyStruct;
    notExpected : PVOID := 0;
    actual : PVOID;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST('TestAddress');

actual := _type.MyBit.Address;

AssertFalse(
    Condition := (notExpected = actual),
    Message := 'Type did not return a valid address.'
);

TEST_FINISHED();
]]></ST>
      </Implementation>
    </Method>
    <Method Name="TestClearReference" Id="{3435f573-ddcd-429a-990f-ab9da5a59bfe}">
      <Declaration><![CDATA[METHOD PUBLIC TestClearReference
VAR
    _type : _MyStruct;
	target : MyStruct;
    expected : BOOL := FALSE;
    actual : BOOL;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST('TestClearReference');

_type.MakeReferenceTo(target);
_type.ClearReference();
target.MyBit := TRUE;

actual := _type.MyBit.Value;

AssertEquals(
    Expected := expected,
    Actual := actual,
    Message := 'Value incorrectly followed target change'
);

TEST_FINISHED();
]]></ST>
      </Implementation>
    </Method>
    <Method Name="TestClone" Id="{4c2de117-d9d9-4cc0-95cb-a8fe2089f37a}">
      <Declaration><![CDATA[METHOD PUBLIC TestClone
VAR
    _type : _MyStruct;
	clone : I_Datatype;
	isValidClone : BOOL;
	compareResult : DINT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TEST('TestClone');

clone := _type.Clone();

isValidClone := clone <> 0;

IF isValidClone THEN
	compareResult := memcmp(_type.Address, clone.Address, clone.Size);
END_IF

AssertTrue(
	Condition := isValidClone,
	Message := 'Clone did not produce a clone'
);

clone.Dispose();

TEST_FINISHED();
]]></ST>
      </Implementation>
    </Method>
